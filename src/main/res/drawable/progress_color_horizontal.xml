<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="5dp"/>
            <gradient android:startColor="#ff9d9e9d" android:endColor="#ff747674" android:angle="270" android:centerY="0.75" android:centerColor="#ff5a5d5a"/>
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="5dp"/>
                <gradient android:startColor="#80c07ab8" android:endColor="#a0c07ab8" android:angle="270" android:centerY="0.75" android:centerColor="#80c07ab8"/>
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="5dp"/>
                <gradient android:startColor="@color/color_blue" android:endColor="@color/color_blue" android:angle="270" android:centerY="0.75" android:centerColor="@color/color_blue"/>
            </shape>
        </clip>
    </item>
</layer-list>
