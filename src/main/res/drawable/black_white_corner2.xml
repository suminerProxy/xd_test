<?xml version="1.0" encoding="utf-8"?>
<inset xmlns:android="http://schemas.android.com/apk/res/android">
    <selector>
        <item android:state_pressed="true">
            <shape>
                <gradient android:startColor="@color/default_system_background" android:endColor="@color/default_system_background" android:angle="270"/>
                <stroke android:width="1dp" android:color="@color/black2"/>
                <corners android:radius="@dimen/normal_btn_radius"/>
            </shape>
        </item>
        <item>
            <shape>
                <gradient android:startColor="@color/color_white" android:endColor="@color/color_white" android:angle="270"/>
                <stroke android:width="1dp" android:color="@color/abc_list_pressed_holo_light"/>
                <corners android:radius="@dimen/normal_btn_radius"/>
            </shape>
        </item>
    </selector>
</inset>
