<?xml version="1.0" encoding="utf-8"?>
<inset xmlns:android="http://schemas.android.com/apk/res/android">
    <selector>
        <item android:state_pressed="false">
            <shape>
                <solid android:color="@color/color_white"/>
                <corners android:radius="@dimen/normal_btn_radius"/>
                <stroke android:width="1dp" android:color="@color/abc_list_pressed_holo_light"/>
            </shape>
        </item>
        <item android:state_pressed="true">
            <shape>
                <solid android:color="@color/abc_list_pressed_holo_light"/>
                <corners android:radius="@dimen/normal_btn_radius"/>
                <stroke android:width="1dp" android:color="@color/abc_list_pressed_holo_light"/>
            </shape>
        </item>
    </selector>
</inset>
