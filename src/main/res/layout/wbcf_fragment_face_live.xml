<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android" android:background="?attr/wbcfFaceVerifyBgColor" android:layout_width="match_parent" android:layout_height="match_parent">
    <RelativeLayout android:layout_width="match_parent" android:layout_height="match_parent">
        <com.tencent.cloud.huiyansdkface.facelight.ui.widget.PreviewFrameLayout android:id="@+id/wbcf_live_preview_layout" android:layout_width="match_parent" android:layout_height="match_parent" android:layout_centerInParent="true"/>
        <com.tencent.cloud.huiyansdkface.facelight.ui.widget.HeadBorderView android:id="@+id/wbcf_live_preview_bottom" android:layout_width="match_parent" android:layout_height="match_parent" android:layout_centerInParent="true"/>
        <com.tencent.cloud.huiyansdkface.facelight.ui.widget.PreviewMask android:id="@+id/wbcf_live_preview_mask" android:visibility="gone" android:layout_width="match_parent" android:layout_height="match_parent" android:layout_centerInParent="true"/>
        <RelativeLayout android:id="@+id/wbcf_toast_height" android:layout_width="match_parent" android:layout_height="1dp">
            <TextView android:textSize="16sp" android:textColor="@color/wbcf_white" android:gravity="center" android:id="@+id/wbcf_network_retry_tip" android:background="@drawable/wbcf_network_retry_tip_bg" android:paddingLeft="24dp" android:paddingTop="12dp" android:paddingRight="24dp" android:paddingBottom="12dp" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_centerInParent="true" android:lineSpacingMultiplier="1.2"/>
        </RelativeLayout>
        <RelativeLayout android:id="@+id/wbcf_back_rl" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="16dp" android:layout_marginTop="26dp" android:layout_marginRight="16dp">
            <ImageView android:id="@+id/wbcf_live_back" android:padding="10dp" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="wrap_content" android:src="@mipmap/wbcf_back" android:layout_alignParentLeft="true"/>
            <ImageView android:id="@+id/wbcf_change_cam_facing" android:padding="10dp" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="wrap_content" android:src="@mipmap/wbcf_change_camera_facing" android:layout_alignParentRight="true"/>
        </RelativeLayout>
        <TextView android:textColor="@color/wbcf_white" android:id="@+id/wbcf_light_percent_tv" android:layout_width="wrap_content" android:layout_height="wrap_content"/>
        <TextView android:textColor="@color/wbcf_white" android:id="@+id/wbcf_light_pyr_tv" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_alignParentRight="true"/>
        <View android:id="@+id/wbcf_command_height" android:layout_width="match_parent" android:layout_height="1dp"/>
        <TextView android:gravity="center" android:id="@+id/wbcf_live_tip_tv" android:visibility="invisible" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginBottom="32dp" android:layout_alignBottom="@+id/wbcf_command_height"/>
        <View android:id="@+id/wbcf_light_height" android:layout_width="match_parent" android:layout_height="1dp"/>
        <TextView android:gravity="center" android:id="@+id/wbcf_customer_tip" android:visibility="invisible" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="32dp" android:layout_alignTop="@+id/wbcf_light_height"/>
        <RelativeLayout android:id="@+id/wbcf_customer_long_tip_bg" android:background="?attr/wbcfCustomerLongTipBg" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginLeft="16dp" android:layout_marginRight="16dp" android:layout_marginBottom="32dp" android:layout_alignParentBottom="true">
            <TextView android:textSize="14sp" android:textColor="?attr/wbcfCustomerLongTipTextColor" android:id="@+id/wbcf_customer_long_tip" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginLeft="16dp" android:layout_marginTop="12dp" android:layout_marginRight="16dp" android:layout_marginBottom="12dp"/>
        </RelativeLayout>
    </RelativeLayout>
</FrameLayout>
