            </LinearLayout>
            <TextView android:textColor="@color/sobot_common_gray1" android:gravity="center" android:layout_gravity="start|center_vertical" android:id="@+id/sobot_evaluate_cancel" android:paddingLeft="10dp" android:paddingRight="10dp" android:visibility="visible" android:layout_width="wrap_content" android:layout_height="match_parent" android:layout_toLeftOf="@+id/sobot_ll_evaluate_title" android:paddingStart="10dp" android:paddingEnd="10dp" android:layout_toStartOf="@+id/sobot_ll_evaluate_title"/>
        </FrameLayout>
        <View android:background="@color/sobot_line_1dp" android:layout_width="match_parent" android:layout_height="0.4dp"/>
        <ScrollView android:scrollbars="none" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_weight="1">
            <LinearLayout android:orientation="vertical" android:id="@+id/sobot_relative" android:paddingTop="@dimen/sobot_DIMEN_48PX" android:paddingBottom="20dp" android:layout_width="match_parent" android:layout_height="wrap_content">
