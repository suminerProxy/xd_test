                <TextView android:textSize="12sp" android:textStyle="bold" android:textColor="#999999" android:ellipsize="end" android:gravity="center" android:id="@+id/sobot_tv_evaluate_title_hint" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="3dp" android:layout_marginBottom="6dp" android:lines="1"/>
            </LinearLayout>
            <LinearLayout android:layout_gravity="end|center_vertical" android:id="@+id/sobot_negativeButton" android:layout_width="60dp" android:layout_height="match_parent">
