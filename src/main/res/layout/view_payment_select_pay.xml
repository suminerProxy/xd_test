<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android" android:orientation="vertical" android:layout_width="match_parent" android:layout_height="wrap_content">
    <RelativeLayout android:id="@+id/payment_alipay" android:paddingLeft="@dimen/dimen_sw_15dp" android:paddingRight="@dimen/dimen_sw_20dp" android:layout_width="match_parent" android:layout_height="@dimen/dimen_sw_55dp">
        <ImageView android:id="@+id/image_icon_zhifubao" android:layout_width="@dimen/dimen_sw_24dp" android:layout_height="@dimen/dimen_sw_24dp" android:src="@drawable/zhifubaopay" android:layout_centerVertical="true" android:contentDescription="@string/app_name"/>
        <TextView android:textSize="13sp" android:textColor="@color/abc_ic_ab_back_material" android:id="@+id/a_pay_str" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="@dimen/dimen_sw_15dp" android:text="@string/pay_alipay" android:layout_toRightOf="@+id/image_icon_zhifubao" android:layout_centerVertical="true"/>
        <TextView android:textSize="12sp" android:textColor="@color/abc_ic_voice_search_api_material" android:id="@+id/a_pay_str_none" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="不可用" android:layout_alignParentRight="true" android:layout_centerVertical="true"/>
        <ImageView android:id="@+id/image_alipay" android:layout_width="18dp" android:layout_height="18dp" android:src="@drawable/icon_gougou_red" android:layout_alignParentRight="true" android:layout_centerVertical="true" android:contentDescription="@string/app_name"/>
    </RelativeLayout>
    <View android:id="@+id/line" android:background="@color/color_line" android:visibility="gone" android:layout_width="match_parent" android:layout_height="0.3dp" android:layout_marginLeft="@dimen/dimen_sw_15dp" android:layout_marginRight="@dimen/dimen_sw_15dp"/>
    <RelativeLayout android:id="@+id/payment_weixin" android:paddingLeft="@dimen/dimen_sw_15dp" android:paddingRight="@dimen/dimen_sw_20dp" android:layout_width="match_parent" android:layout_height="@dimen/dimen_sw_55dp">
        <ImageView android:id="@+id/image_icon_weixin" android:layout_width="@dimen/dimen_sw_24dp" android:layout_height="@dimen/dimen_sw_24dp" android:src="@drawable/weixinpay" android:layout_centerVertical="true" android:contentDescription="@string/app_name"/>
        <TextView android:textSize="13sp" android:textColor="@color/abc_ic_ab_back_material" android:id="@+id/w_pay_str" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="@dimen/dimen_sw_15dp" android:text="@string/pay_wechat" android:layout_toRightOf="@+id/image_icon_weixin" android:layout_centerVertical="true"/>
        <TextView android:textSize="12sp" android:textColor="@color/abc_ic_voice_search_api_material" android:id="@+id/w_pay_str_none" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="不可用" android:layout_alignParentRight="true" android:layout_centerVertical="true"/>
        <ImageView android:id="@+id/image_weixin" android:layout_width="18dp" android:layout_height="18dp" android:src="@drawable/pay_unselect" android:layout_alignParentRight="true" android:layout_centerVertical="true" android:contentDescription="@string/app_name"/>
    </RelativeLayout>
    <View android:id="@+id/line_bo" android:background="@color/color_line" android:visibility="visible" android:layout_width="match_parent" android:layout_height="0.3dp" android:layout_marginLeft="@dimen/dimen_sw_15dp" android:layout_marginRight="@dimen/dimen_sw_15dp" android:layout_marginBottom="@dimen/dimen_sw_14dp"/>
    <LinearLayout android:orientation="vertical" android:id="@+id/ll_state" android:paddingBottom="@dimen/dimen_sw_10dp" android:visibility="visible" android:layout_width="match_parent" android:layout_height="match_parent">
        <TextView android:textSize="12sp" android:textColor="#aaaaaa" android:ellipsize="end" android:id="@+id/ticketDesTv" android:paddingLeft="@dimen/dimen_sw_15dp" android:paddingRight="@dimen/dimen_sw_15dp" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="@dimen/dimen_sw_10dp" android:text="@string/crowdfunding_notice_new" android:lineSpacingExtra="3dp"/>
        <View android:id="@+id/lins" android:layout_width="match_parent" android:layout_height="0.3dp" android:layout_marginLeft="@dimen/dimen_sw_15dp" android:layout_marginTop="@dimen/dimen_sw_15dp" android:layout_marginRight="@dimen/dimen_sw_15dp" style="@style/lineHorizontalEB"/>
        <TextView android:textSize="13sp" android:textColor="@color/abc_ic_star_half_black_36dp" android:id="@+id/ticket_phone_hint" android:padding="15dp" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="10dp" android:text="@string/call_service" android:drawableRight="@drawable/mine_icon_next"/>
    </LinearLayout>
</merge>
