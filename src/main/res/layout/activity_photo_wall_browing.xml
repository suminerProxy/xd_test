<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" android:orientation="vertical" android:background="#121212" android:layout_width="match_parent" android:layout_height="match_parent">
    <LinearLayout android:orientation="vertical" android:layout_width="match_parent" android:layout_height="wrap_content">
        <com.taihebase.activity.view.TopBarEmptyView android:background="#000000" android:layout_width="match_parent" android:layout_height="wrap_content"/>
        <FrameLayout android:background="#000000" android:layout_width="match_parent" android:layout_height="@dimen/dimen_sw_44dp">
            <ImageView android:id="@+id/iv_back" android:paddingLeft="@dimen/dimen_sw_15dp" android:paddingRight="@dimen/dimen_sw_15dp" android:layout_width="wrap_content" android:layout_height="match_parent" android:src="@drawable/b_back_white"/>
            <TextView android:textSize="@dimen/dimen_sw_16sp" android:textColor="@color/white" android:ellipsize="end" android:layout_gravity="center" android:id="@+id/tv_Title" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="@dimen/dimen_sw_40dp" android:layout_marginRight="@dimen/dimen_sw_40dp" android:maxLines="1"/>
        </FrameLayout>
        <com.taihe.photoselector.ui.photodrawee.MultiTouchViewPager android:id="@+id/vp_viewpager" android:layout_width="match_parent" android:layout_height="0dp" android:layout_marginBottom="@dimen/dimen_sw_100dp" android:layout_weight="1"/>
    </LinearLayout>
    <com.showstartfans.activity.view.ShopMiniBarView android:id="@+id/shopMoniBar" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginBottom="@dimen/dimen_sw_32dp" android:layout_alignParentBottom="true" android:layout_centerHorizontal="true"/>
</RelativeLayout>
