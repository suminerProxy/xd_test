    <TextView android:textSize="12sp" android:textColor="@color/abc_ic_star_half_black_36dp" android:id="@+id/crowSelectDes" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginLeft="@dimen/dimen_sw_20dp" android:layout_marginRight="@dimen/dimen_sw_20dp" android:layout_marginBottom="@dimen/dimen_sw_15dp" android:layout_below="@+id/view_t" android:lineSpacingExtra="8dp"/>
    <com.showstartfans.activity.view.PhotoGridLayout android:id="@+id/photoGridView" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginLeft="@dimen/dimen_sw_20dp" android:layout_marginRight="@dimen/dimen_sw_20dp" android:layout_marginBottom="@dimen/dimen_sw_15dp" android:layout_below="@+id/crowSelectDes" app:show_image_type="2" app:space="10dp"/>
    <LinearLayout android:id="@+id/f_p" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginLeft="@dimen/dimen_sw_20dp" android:layout_marginRight="@dimen/dimen_sw_20dp" android:layout_below="@+id/photoGridView">
