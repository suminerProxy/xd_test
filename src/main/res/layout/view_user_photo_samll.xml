<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" android:layout_width="wrap_content" android:layout_height="wrap_content">
    <FrameLayout android:orientation="vertical" android:layout_width="@dimen/dimen_sw_55dp" android:layout_height="wrap_content">
        <FrameLayout android:background="@drawable/shape_5dp_black" android:layout_width="match_parent" android:layout_height="@dimen/dimen_sw_55dp" android:layout_marginTop="@dimen/dimen_sw_27dp">
            <ImageView android:layout_gravity="bottom|center_horizontal" android:id="@+id/iv_small_icon" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginBottom="@dimen/dimen_sw_7dp" android:src="@mipmap/icon_add_mine"/>
        </FrameLayout>
        <FrameLayout android:background="@drawable/bg_shadow" android:layout_width="match_parent" android:layout_height="wrap_content">
            <com.facebook.drawee.view.SimpleDraweeView android:id="@+id/smal_Conver" android:layout_width="@dimen/dimen_sw_55dp" android:layout_height="@dimen/dimen_sw_55dp" app:roundedCornerRadius="4dp"/>
            <ImageView android:id="@+id/smal_Conver_normal" android:layout_width="@dimen/dimen_sw_55dp" android:layout_height="@dimen/dimen_sw_55dp" android:src="@mipmap/image_empty"/>
            <View android:id="@+id/v_over_layer" android:background="#40000000" android:layout_width="@dimen/dimen_sw_55dp" android:layout_height="@dimen/dimen_sw_55dp"/>
            <TextView android:textSize="@dimen/dimen_sw_18sp" android:textColor="@color/white" android:layout_gravity="center" android:id="@+id/tv_photo_num" android:layout_width="wrap_content" android:layout_height="wrap_content"/>
        </FrameLayout>
    </FrameLayout>
</FrameLayout>
